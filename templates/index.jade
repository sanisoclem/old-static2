
extends layout

block content
  .jc-home
    .region-top=contents["cv.json"].metadata.firstName + " " + contents["cv.json"].metadata.lastName
    .region-bottom
      ul.social
        each link, i in contents["cv.json"].metadata.social
          li
            a(href=link.url)
              i(class="fa fa-link")
              span=link.type  